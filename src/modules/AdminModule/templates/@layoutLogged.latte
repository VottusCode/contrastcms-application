<!doctype html>
<html>

<head>
    <title>Contrast CMS 4.6</title>

    <meta charset="utf-8">
    <!--[if lt IE 9]>
    <script src="{$basePath}/js/libs/html5shiv.js"></script><![endif]-->
    <link rel="stylesheet" type="text/css" href="{$basePath}/backend/css/style.css"/>
    <link type="text/css" href="{$basePath}/backend/css/jquery-ui-1.10.1.custom.css" rel="stylesheet"/>

    <script type="text/javascript">
        var imagesGetter = {plink File:imagesJson};
        var imageUploadUrl = {plink File:imageUploadAction};
        var fileUploadUrl = {plink File:fileUploadAction};
        var treeUrl =  {plink Post:moveItem};
        var basePath =  {$basePath};
        var treeStoageUrl =  {plink Post:saveCollapsion};
        var galleryAjaxUploadUrl =  {plink Gallery:ajaxUpload};
        var galleryDefaultUrl =  {plink Gallery:default};
    </script>
</head>

<body>
<div id="page" oncontextmenu="return false;">

    <header>
        <h1>CONTRAST <strong>CMS 4.6</strong></h1>
        <div class="user">
            <span class="divider">&nbsp;</span>

            <p>Welcome <strong>{$userData['name']} {$userData['surname']}</strong></p>
            <a n:href="Sign:logout">logout</a>
        </div>
    </header>

    <hr/>

    <section>
        <div class="right_column full">
            {control adminMenu}
            <hr/>
            {include #content}
        </div>
    </section>

</div>

<div n:if="$flashes" id="flashes">
    <div n:foreach="$flashes as $flash" class="flash {$flash->type}">{$flash->message}</div>
</div>

{block scripts}
    <script type="text/javascript" src="{$basePath}/js/libs/jquery.min.js?v={$version}"></script>
    <script src="{$basePath}/js/libs/netteForms.js?v={$version}"></script>
    <script src="{$basePath}/js/contrast/admin.js?v={$version}"></script>
    <script src="{$basePath}/backend/ckeditor/ckeditor.js?v={$version}"></script>
    <script type="text/javascript" src="{$basePath}/js/libs/jquery-ui-1.10.0.custom.min.js"></script>
    <script src="{$basePath}/js/libs/jquery.nestable.js"></script>
{/block}

</body>

</html>


<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, shrink-to-fit=no">
    <title>Espire - Bootstrap 4 Admin Template</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="https://spacek.digital/cms/images/logo/favicon.png">

    <!-- plugins css -->
    <link rel="stylesheet" href="https://spacek.digital/cms/vendor/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="https://spacek.digital/cms/vendor/PACE/themes/blue/pace-theme-minimal.css" />
    <link rel="stylesheet" href="https://spacek.digital/cms/vendor/perfect-scrollbar/css/perfect-scrollbar.min.css" />

    <!-- core css -->
    <link href="{$basePath}/cms/dist/assets/css/ei-icon.css" rel="stylesheet">
    <link href="{$basePath}/cms/dist/assets/css/themify-icons.css" rel="stylesheet">
    <link href="{$basePath}/cms/dist/assets/css/font-awesome.min.css" rel="stylesheet">
    <link href="https://spacek.digital/cms/css/animate.min.css" rel="stylesheet">
    <link href="https://spacek.digital/cms/css/app.css" rel="stylesheet">
</head>

<body>
<div class="app">
    <div class="layout">
        <!-- Side Nav START -->
        <div class="side-nav">
            <div class="side-nav-inner">
                <div class="side-nav-logo">
                    <a href="index.html">
                        <div class="logo logo-dark"><h3 style="padding: 13px 0;">Contrast CMS</h3></div>
                        <div class="logo logo-white"><h3 style="padding: 13px 0;">Contrast CMS</h3></div>
                    </a>
                    <div class="mobile-toggle side-nav-toggle">
                        <a href="">
                            <i class="ti-arrow-circle-left"></i>
                        </a>
                    </div>
                </div>
                <ul class="side-nav-menu scrollable">
                    {control adminMenu}
                </ul>
            </div>
        </div>
        <!-- Side Nav END -->

        <!-- Page Container START -->
        <div class="page-container">
            <!-- Header START -->
            <div class="header navbar">
                <div class="header-container">
                    <ul class="nav-left">
                        <li class="search-box">
                            <a class="search-toggle no-pdd-right" href="javascript:void(0);">
                                <i class="search-icon ti-search pdd-right-10"></i>
                                <i class="search-icon-close ti-close pdd-right-10"></i>
                            </a>
                        </li>
                        <li class="search-input">
                            <input class="form-control" type="text" placeholder="Search...">
                            <div class="advanced-search">
                                <div class="search-wrapper">
                                    <div class="pdd-vertical-10">
                                            <span class="display-block mrg-vertical-5 pdd-horizon-20 text-gray">
													<i class="ti-user pdd-right-5"></i>
													<span>People</span>
                                            </span>
                                        <ul class="list-unstyled list-info">
                                            <li>
                                                <a href="">
                                                    <img class="thumb-img" src="https://spacek.digital/cms/images/avatars/thumb-1.jpg" alt="">
                                                    <div class="info">
                                                        <span class="title">Jordan Hurst</span>
                                                        <span class="sub-title">
																	<i class="ti-location-pin"></i>
																	<span>44 Shirley Ave. West Chicago</span>
                                                            </span>
                                                    </div>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="">
                                                    <img class="thumb-img" src="https://spacek.digital/cms/images/avatars/thumb-5.jpg" alt="">
                                                    <div class="info">
                                                        <span class="title">Jennifer Watkins</span>
                                                        <span class="sub-title">
																	<i class="ti-location-pin"></i>
																	<span>514 S. Magnolia St. Orlando</span>
                                                            </span>
                                                    </div>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="">
                                                    <img class="thumb-img" src="https://spacek.digital/cms/images/avatars/thumb-4.jpg" alt="">
                                                    <div class="info">
                                                        <span class="title">Michael Birch</span>
                                                        <span class="sub-title">
																	<i class="ti-location-pin"></i>
																	<span>70 Bowman St. South Windsor</span>
                                                            </span>
                                                    </div>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="mrg-horizon-20 border top"></div>
                                    <div class="pdd-vertical-10">
                                            <span class="display-block mrg-vertical-5 pdd-horizon-20 text-gray">
													<i class="ti-rss pdd-right-5"></i>
													<span>Post</span>
                                            </span>
                                        <ul class="list-unstyled list-info">
                                            <li>
                                                <a href="">
                                                    <img class="thumb-img" src="https://spacek.digital/cms/images/img-1.jpg" alt="">
                                                    <div class="info">
                                                        <span class="title">Artoo expresses his relief</span>
                                                        <span class="sub-title">
																	<span>Oh, thank goodness we're coming out...</span>
                                                            </span>
                                                    </div>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="">
                                                    <img class="thumb-img" src="https://spacek.digital/cms/images/img-2.jpg" alt="">
                                                    <div class="info">
                                                        <span class="title">Ready for some power?</span>
                                                        <span class="sub-title">
																	<span>Lord Vader. You may take Caption So...</span>
                                                            </span>
                                                    </div>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="search-footer">
                                    <span>You are Searching for '<b class="text-dark"><span class="serach-text-bind"></span></b>'</span>
                                </div>
                            </div>
                        </li>
                    </ul>
                    <ul class="nav-right">
                        <li class="user-profile dropdown">
                            <a href="" class="dropdown-toggle" data-toggle="dropdown">
                                <img class="profile-img img-fluid" src="https://spacek.digital/cms/images/user.jpg" alt="">
                                <div class="user-info">
                                    {var $userData = $user->getIdentity()->getData()}
                                    <span class="name pdd-right-5">{$userData['name']} {$userData['surname']}</span>
                                    <i class="ti-angle-down font-size-10"></i>
                                </div>
                            </a>
                            <ul class="dropdown-menu">
                                {*<li>
                                    <a href="">
                                        <i class="ti-settings pdd-right-10"></i>
                                        <span>Setting</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="">
                                        <i class="ti-user pdd-right-10"></i>
                                        <span>Profile</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="">
                                        <i class="ti-email pdd-right-10"></i>
                                        <span>Inbox</span>
                                    </a>
                                </li>
                                <li role="separator" class="divider"></li>*}
                                <li>
                                    <a n:href="Sign:logout">
                                        <i class="ti-power-off pdd-right-10"></i>
                                        <span>Logout</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="notifications dropdown">
                            <span class="counter">2</span>
                            <a href="" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="ti-bell"></i>
                            </a>

                            <ul class="dropdown-menu ">
                                <li class="notice-header">
                                    <i class="ti-bell pdd-right-10"></i>
                                    <span>Notifikace</span>
                                </li>
                                <li>
                                    <ul class="list-info overflow-y-auto relative scrollable">
                                        {*<li>
                                            <a href="">
                                                <img class="thumb-img" src="https://spacek.digital/cms/images/avatars/thumb-5.jpg" alt="">
                                                <div class="info">
                                                        <span class="title">
															<span class="font-size-14 text-semibold">Jennifer Watkins</span>
                                                        <span class="text-gray">commented on your <span class="text-dark">post</span></span>
                                                        </span>
                                                    <span class="sub-title">5 mins ago</span>
                                                </div>
                                            </a>
                                        </li>*}
                                    </ul>
                                </li>

                                <li class="notice-footer">
                                        <span>
                                            <a href="" class="text-gray">Check all notifications <i class="ei-right-chevron pdd-left-5 font-size-10"></i></a>
										</span>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
            <!-- Header END -->

            <!-- Content Wrapper START -->
            <div class="main-content">
                <div class="container-fluid">
                    {include #content}
                </div>
            </div>
            <!-- Content Wrapper END -->

        </div>
        <!-- Page Container END -->

    </div>
</div>

<!-- build:js https://spacek.digital/cms/js/vendor.js -->
<!-- plugins js -->
<script src="https://spacek.digital/cms/vendor/jquery/dist/jquery.min.js"></script>
<script src="https://spacek.digital/cms/vendor/popper.js/dist/umd/popper.min.js"></script>
<script src="https://spacek.digital/cms/vendor/bootstrap/dist/js/bootstrap.js"></script>
<script src="https://spacek.digital/cms/vendor/PACE/pace.min.js"></script>
